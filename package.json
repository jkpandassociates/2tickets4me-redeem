{
  "name": "twotixredeem",
  "version": "0.1.0",
  "license": "ISC",
  "scripts": {
    "postinstall": "yarn run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "debug": "nodemon --debug -e ts --exec \"yarn run build && ts-node\" -- --project server server/index.ts",
    "debug-brk": "nodemon --debug-brk -e ts --exec \"yarn run build && ts-node\" -- --project server server/index.ts",
    "serve": "concurrently \"yarn run debug\" \"yarn run ng-serve\"",
    "ng-serve": "ng serve --proxy-config proxy.conf.json -o",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "tsc -p server && ng build --prod",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "private": true,
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.30",
    "@angular/compiler-cli": "^2.3.1",
    "@types/confidence": "^1.4.30",
    "@types/hapi": "^16.0.0",
    "@types/hapi-decorators": "^0.4.2",
    "@types/jasmine": "2.5.38",
    "@types/joi": "^10.0.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.52",
    "@types/sequelize": "^3.4.36",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.2.0",
    "conventional-changelog": "^1.1.0",
    "cz-conventional-changelog": "^1.2.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "nodemon": "^1.11.0",
    "protractor": "~4.0.13",
    "source-map-loader": "^0.1.6",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "confidence": "^3.0.2",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "glue": "^4.1.0",
    "good": "^7.1.0",
    "good-console": "^6.2.0",
    "good-squeeze": "^5.0.1",
    "hapi": "^16.1.0",
    "hapi-decorators": "^0.4.3",
    "inert": "^4.1.0",
    "joi": "^10.2.1",
    "moment": "^2.17.1",
    "mysql": "^2.13.0",
    "rxjs": "^5.0.1",
    "sequelize": "^3.30.1",
    "sparkpost": "^2.1.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
