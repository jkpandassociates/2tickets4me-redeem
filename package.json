    "@angular/compiler": "^5.0.5",
    "@angular/core": "^5.0.5",
    "@angular/forms": "^5.0.5",
    "@angular/http": "^5.0.5",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.5",
    "@angular/platform-browser-dynamic": "^5.0.5",
    "@angular/router": "^5.0.5",
    "confidence": "^3.0.2",
    "core-js": "^2.5.1",
    "dotenv": "^4.0.0",
    "glue": "^4.2.1",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.6.2",
    "hapi-decorators": "^0.4.3",
    "inert": "^4.2.1",
    "joi": "^10.6.0",
    "moment": "^2.19.3",
